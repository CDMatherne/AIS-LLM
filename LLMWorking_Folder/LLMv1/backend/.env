# Console Debugging Configuration
LOG_LEVEL=DEBUG
ENABLE_CONSOLE_LOGGING=true
LOG_FORMAT=detailed
ENABLE_FILE_LOGGING=true

# Required API Key
CLAUDE_API_KEY=sk-ant-ap...

# Data Source (local)
DATA_SOURCE=local
LOCAL_DATA_PATH=./data

# Output Directory
OUTPUT_DIRECTORY=./output

# Debugging Flags
VERBOSE_ERRORS=true
LOG_REQUESTS=true
LOG_WEBSOCKET=true
LOG_TOOL_EXECUTION=true
